<!DOCTYPE html>
<html lang="en">

<head>
	<%- include('partials/head'); -%>
</head>

<body>
	<%- include('partials/nav'); -%>
	<main>
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<div class="error-template">

						<% if (typeof(error) !=="undefined"){ %>

						<h1>
							<%= error.status %> </h1>
						<h2>
							<%= error.message %> </h2>
						<div class="error-details">
							<%= error.detail %>
						</div>

						<% } %>

					</div>
				</div>
			</div>
		</div>
	</main>
</body>

</html>